@model AnimalDB.Repo.Entities.CageLocationHistory

<h3>
    @Html.ActionLink("Back", "Index", "CageLocationHistory", new { id = Model.Animal_Id }, new { @class = "btn btn-primary" })
</h3>

<h2>Delete</h2>

<h3>Are you sure you want to delete this cage location?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Timestamp)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Timestamp)
        </dd>

        @if (Model.CageLocation_Id != null)
        {

            <dt>
                @Html.DisplayNameFor(model => model.CageLocation_Id)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CageLocation.Description)
            </dd>

        }
        else
        {
            <dt>
                @Html.DisplayNameFor(model => model.RackEntry.Reference)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.RackEntry.Reference)
            </dd>
        }

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn  btn-danger" />
        </div>
    }
</div>
