@model AnimalDB.Repo.Entities.VetSchedule

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete the vet schedule?</h3>
<div>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-outline-danger" />
            @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-outline-dark " })
        </div>
    }
</div>
