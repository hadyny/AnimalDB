@model AnimalDB.Repo.Entities.SurgicalNote

<h3>
    @Html.ActionLink("Back", "Index", "SurgicalNote", new { id = Model.Animal_Id }, new { @class = "btn btn-primary" })
</h3>

<h3>Are you sure you want to delete this surgical note?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Timestamp)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Timestamp)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.SurgeryType)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SurgeryType.Description)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Surgeon)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Surgeon)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn  btn-outline-danger" />
        </div>
    }
</div>
